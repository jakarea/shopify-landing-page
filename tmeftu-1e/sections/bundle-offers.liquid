{% assign paper_pro_product = all_products[section.settings.paper_pro_product] %}
<script>
  (function() {
    const products = [];
    {% if ultimate_product != blank %}
      {% assign ultimate_variant = ultimate_product.selected_or_first_available_variant %}
      products.push({
        variantId: {{ ultimate_variant.id }},
        price: {{ ultimate_variant.price | divided_by: 100.00 }},
        hasCompare: {% if ultimate_variant.compare_at_price > ultimate_variant.price %}true{% else %}false{% endif %},
        comparePrice: {% if ultimate_variant.compare_at_price > ultimate_variant.price %}{{ ultimate_variant.compare_at_price | divided_by: 100.00 }}{% else %}0{% endif %}
      });
    {% endif %}
    {% if paper_pro_product != blank %}
      {% assign paper_pro_variant = paper_pro_product.selected_or_first_available_variant %}
      products.push({
        variantId: {{ paper_pro_variant.id }},
        price: {{ paper_pro_variant.price | divided_by: 100.00 }},
        hasCompare: {% if paper_pro_variant.compare_at_price > paper_pro_variant.price %}true{% else %}false{% endif %},
        comparePrice: {% if paper_pro_variant.compare_at_price > paper_pro_variant.price %}{{ paper_pro_variant.compare_at_price | divided_by: 100.00 }}{% else %}0{% endif %}
      });
    {% endif %}
    if (products.length === 0) {
      return;
    }
    // Use centralized EUR price cache
    function getEurPrice(variantId, fallbackPrice) {
      if (typeof window.EURPriceCache !== 'undefined') {
        const cachedPrice = window.EURPriceCache.getPrice(variantId);
        if (cachedPrice && cachedPrice > 0) {
          return cachedPrice;
        }
      }
      return fallbackPrice;
    }
    // Get compare price from cache or fallback
    function getEurComparePrice(variantId, fallbackPrice) {
      if (typeof window.EURPriceCache !== 'undefined' && window.EURPriceCache.getComparePrice) {
        const cachedComparePrice = window.EURPriceCache.getComparePrice(variantId);
        if (cachedComparePrice && cachedComparePrice > 0) {
          return cachedComparePrice;
        }
      }
      return fallbackPrice;
    }
    // Format and display price for each product
    function displayBundlePrices() {
      const currentCurrency = (typeof window.CurrencyManager !== 'undefined')
        ? window.CurrencyManager.getCurrentCurrency()
        : 'EUR';
      products.forEach((product) => {
        const eurPrice = getEurPrice(product.variantId, product.price);
        const priceEls = document.querySelectorAll('.bundle-price[data-variant-id="' + product.variantId + '"]');
        const compareEls = document.querySelectorAll('.bundle-original[data-variant-id="' + product.variantId + '-compare"]');
        priceEls.forEach(priceEl => {
          if (priceEl && eurPrice > 0) {
            priceEl.dataset.priceEur = eurPrice;
            priceEl.dataset.priceBase = 'EUR';
            priceEl.classList.remove('price-loading');
            if (typeof window.CurrencyManager !== 'undefined' && window.CurrencyManager.formatPrice) {
              const formatted = window.CurrencyManager.formatPrice(eurPrice, currentCurrency);
              // Only update if formatted price is valid (not 0 or empty)
              if (formatted && !formatted.match(/^[\€\$£¥]\s*0$/)) {
                priceEl.textContent = formatted;
              }
            } else {
              priceEl.textContent = '€' + Math.round(eurPrice);
            }
          }
        });
        if (product.hasCompare && product.comparePrice > 0) {
          const compareEurPrice = getEurComparePrice(product.variantId, product.comparePrice);
          compareEls.forEach(compareEl => {
            if (compareEl) {
              compareEl.dataset.priceEur = compareEurPrice;
              compareEl.dataset.priceBase = 'EUR';
              compareEl.classList.remove('price-loading');
              if (typeof window.CurrencyManager !== 'undefined' && window.CurrencyManager.formatPrice) {
                compareEl.textContent = window.CurrencyManager.formatPrice(compareEurPrice, currentCurrency);
              } else {
                compareEl.textContent = '€' + Math.round(compareEurPrice);
              }
            }
          });
        }
      });
    }
    // Try to display prices immediately
    displayBundlePrices();
    // If CurrencyManager is loading, wait for it to be ready
    if (typeof window.CurrencyManager !== 'undefined') {
      const rates = window.CurrencyManager.getRates();
      if (!rates || Object.keys(rates).length === 0) {
        let attempts = 0;
        const maxAttempts = 30; // 3 seconds max
        const checkInterval = setInterval(() => {
          attempts++;
          const newRates = window.CurrencyManager.getRates();
          if (newRates && Object.keys(newRates).length > 0) {
            clearInterval(checkInterval);
            displayBundlePrices();
          } else if (attempts >= maxAttempts) {
            clearInterval(checkInterval);
            displayBundlePrices();
          }
        }, 100);
      }
    }
    // Listen for currency changes and re-format prices
    window.addEventListener('currencyChanged', function(e) {
      displayBundlePrices();
    });
  })();
</script>
<script>
  // Bundle Carousel Initialization - Runs when section loads
  (function() {
    // Initialize all bundle carousels on the page
    const carousels = document.querySelectorAll('.bundle-carousel');
    carousels.forEach(function(carousel) {
      const container = carousel.querySelector('.carousel-container');
      if (!container) {
        return;
      }
      const items = container.querySelectorAll('.carousel-item');
      if (items.length === 0) return;
      // Initialize current index
      if (!container.dataset.currentIndex) {
        container.dataset.currentIndex = 0;
      }
      // Set first item as active
      items.forEach(function(item, index) {
        if (index === 0) {
          item.classList.add('active');
        } else {
          item.classList.remove('active');
        }
      });
      // Arrow button click handlers
      const prevBtn = carousel.querySelector('.carousel-arrow.prev');
      const nextBtn = carousel.querySelector('.carousel-arrow.next');
      if (prevBtn) {
        prevBtn.onclick = function() {
          const totalItems = items.length;
          let currentIndex = parseInt(container.dataset.currentIndex || 0);
          const oldIndex = currentIndex;
          currentIndex = (currentIndex - 1 + totalItems) % totalItems;
          container.dataset.currentIndex = currentIndex;
          items[oldIndex].classList.remove('active');
          items[currentIndex].classList.add('active');
        };
      }
      if (nextBtn) {
        nextBtn.onclick = function() {
          const totalItems = items.length;
          let currentIndex = parseInt(container.dataset.currentIndex || 0);
          const oldIndex = currentIndex;
          currentIndex = (currentIndex + 1) % totalItems;
          container.dataset.currentIndex = currentIndex;
          items[oldIndex].classList.remove('active');
          items[currentIndex].classList.add('active');
        };
      }
      // Touch/Swipe support for mobile
      let touchStartX = 0;
      let touchEndX = 0;
      const minSwipeDistance = 50;
      container.addEventListener('touchstart', function(e) {
        touchStartX = e.changedTouches[0].screenX;
      }, { passive: true });
      container.addEventListener('touchend', function(e) {
        touchEndX = e.changedTouches[0].screenX;
        handleSwipe();
      }, { passive: true });
      function handleSwipe() {
        const swipeDistance = touchEndX - touchStartX;
        if (Math.abs(swipeDistance) < minSwipeDistance) {
          return; // Not a swipe
        }
        const totalItems = items.length;
        let currentIndex = parseInt(container.dataset.currentIndex || 0);
        const oldIndex = currentIndex;
        if (swipeDistance > 0) {
          // Swiped right - go to previous
          currentIndex = (currentIndex - 1 + totalItems) % totalItems;
        } else {
          // Swiped left - go to next
          currentIndex = (currentIndex + 1) % totalItems;
        }
        container.dataset.currentIndex = currentIndex;
        items[oldIndex].classList.remove('active');
        items[currentIndex].classList.add('active');
      }
    });
  })();
</script>
