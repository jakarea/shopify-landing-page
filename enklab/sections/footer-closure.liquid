<section class="final-cta">
  <div class="container">
    <div class="content">
      <h2 data-translate="footer_closure_final_cta_title">{{ 'footer_closure.final_cta_title' | t }}</h2>
      <p data-translate="footer_closure_final_cta_desc">{{ 'footer_closure.final_cta_desc' | t }}</p>
      {% assign product = all_products[section.settings.product] %}
      {% if product != blank %}
        {% assign current_variant = product.selected_or_first_available_variant %}
        <button type="button" class="btn footer-add-to-cart" data-product-id="{{ current_variant.id }}" data-variant-id="{{ current_variant.id }}">
          <i class="fas fa-gift"></i> <span data-translate="footer_closure_final_cta_button">{{ 'footer_closure.final_cta_button' | t }}</span> <span class="price price-loading" data-variant-id="{{ current_variant.id }}">
            <span class="price-spinner"><i class="fas fa-spinner fa-spin"></i></span>
          </span> <i class="fas fa-arrow-right"></i>
        </button>
      {% else %}
        <div style="background: #fee; border: 2px solid #c00; border-radius: 8px; padding: 1rem; margin: 1rem 0;">
          <div style="color: #c00; font-weight: bold; margin-bottom: 0.5rem;">
            <i class="fas fa-exclamation-triangle"></i> {{ 'footer_closure.admin_title' | t }}
          </div>
          <p style="color: #900; margin: 0;">
            {{ 'footer_closure.admin_desc' | t }}
            <strong>{{ 'footer_closure.admin_customize' | t }}</strong>
          </p>
        </div>
        <button class="btn" onclick="alert(window.getTranslation('footer_closure_admin_alert') || {{ 'footer_closure.admin_alert' | t | json }})">
          <i class="fas fa-gift"></i> <span data-translate="footer_closure_final_cta_button">{{ 'footer_closure.final_cta_button' | t }}</span> <span class="price" data-price-eur="45">€45</span> <i class="fas fa-arrow-right"></i>
        </button>
      {% endif %}
    </div>
  </div>
</section>
<script>
  (function() {
    // FAQ Accordion Functionality
    document.addEventListener('DOMContentLoaded', function() {
      const faqItems = document.querySelectorAll('.faq-item');
      faqItems.forEach(function(item) {
        const question = item.querySelector('.faq-question');
        const icon = item.querySelector('.faq-question i');
        const answer = item.querySelector('.faq-answer');
        question.addEventListener('click', function() {
          const isOpen = item.classList.contains('open');
          // Close all other FAQs
          faqItems.forEach(function(otherItem) {
            if (otherItem !== item) {
              otherItem.classList.remove('open');
              const otherIcon = otherItem.querySelector('.faq-question i');
              const otherAnswer = otherItem.querySelector('.faq-answer');
              if (otherIcon) otherIcon.style.transform = 'rotate(0deg)';
              if (otherAnswer) otherAnswer.style.display = 'none';
            }
          });
          // Toggle current FAQ
          if (!isOpen) {
            item.classList.add('open');
            if (icon) icon.style.transform = 'rotate(90deg)';
            if (answer) answer.style.display = 'block';
          } else {
            item.classList.remove('open');
            if (icon) icon.style.transform = 'rotate(0deg)';
            if (answer) answer.style.display = 'none';
          }
        });
      });
    });
    const addToCartButton = document.querySelector('.footer-add-to-cart');
    if (!addToCartButton) {
    }
    // Get product data if button exists
    {% assign product = all_products[section.settings.product] %}
    {% if product != blank %}
      {% assign current_variant = product.selected_or_first_available_variant %}
      const productData = {
        id: {{ current_variant.id }},
        name: "{{ product.title | escape }}",
        price: {{ current_variant.price | divided_by: 100.00 }},
        variantTitle: "{{ current_variant.title | escape }}"
      };
      // ============================================================
      // GET EUR PRICE FROM CENTRALIZED CACHE
      // ============================================================
      function getEurPrice(variantId) {
        if (typeof window.EURPriceCache !== 'undefined') {
          const cachedPrice = window.EURPriceCache.getPrice(variantId);
          if (cachedPrice) {
            return cachedPrice;
          }
        }
        return {{ current_variant.price | divided_by: 100.00 }};
      }
      // Initialize the price on page load
      const priceEl = addToCartButton ? addToCartButton.querySelector('.price[data-variant-id]') : null;
      if (priceEl && priceEl.dataset.variantId) {
        const variantId = priceEl.dataset.variantId;
        const eurPrice = getEurPrice(variantId);
        priceEl.dataset.priceEur = eurPrice;
        priceEl.dataset.priceBase = 'EUR';
        priceEl.classList.remove('price-loading');
        if (typeof window.CurrencyManager !== 'undefined') {
          const waitForRates = setInterval(() => {
            const rates = window.CurrencyManager.getRates();
            const currentCurrency = window.CurrencyManager.getCurrentCurrency();
            // Check if rates are ready (either has multiple currencies or at least base rate)
            if (rates && (Object.keys(rates).length > 1 || (rates.EUR && rates.EUR === 1) || currentCurrency === 'EUR')) {
              clearInterval(waitForRates);
              const formattedPrice = window.CurrencyManager.formatPrice(eurPrice, currentCurrency);
              priceEl.textContent = formattedPrice;
            }
          }, 100);
          // Timeout after 5 seconds - fallback to display anyway
          setTimeout(() => {
            clearInterval(waitForRates);
            if (priceEl && priceEl.classList.contains('price-loading')) {
              const currentCurrency = window.CurrencyManager.getCurrentCurrency();
              const formattedPrice = window.CurrencyManager.formatPrice(eurPrice, currentCurrency);
              priceEl.textContent = formattedPrice;
            }
          }, 5000);
        } else {
          // No CurrencyManager, display EUR price directly
          if (priceEl) {
            priceEl.textContent = '€' + eurPrice;
            priceEl.classList.remove('price-loading');
          }
        }
      }
      // Handle add to cart
      if (addToCartButton) {
        addToCartButton.addEventListener('click', function(e) {
          e.preventDefault();
          const originalButtonText = this.innerHTML;
          if (typeof productData === 'undefined') {
            alert({{ 'footer_closure.configure_warning' | t | json }});
            return;
          }
          // Show loading state
          this.innerHTML = '<i class="fas fa-spinner fa-spin"></i> {{ 'footer_closure.adding' | t }}';
          this.disabled = true;
          // Use new Shopify AJAX addToCart function (variant ID only)
          if (typeof addToCart === 'function') {
            addToCart(productData.id);
            // Show success state
            setTimeout(function() {
              addToCartButton.innerHTML = '<i class="fas fa-check"></i> {{ 'footer_closure.added' | t }}';
            }, 500);
            setTimeout(function() {
              addToCartButton.innerHTML = originalButtonText;
              addToCartButton.disabled = false;
            }, 2000);
          } else {
            alert({{ 'footer_closure.add_error' | t | json }});
            this.innerHTML = originalButtonText;
            this.disabled = false;
          }
        });
      }
    {% endif %}
  })();
</script>
<style>
  .faq-item {
    background: var(--card);
    border: 1px solid var(--border);
    border-radius: 0.75rem;
    overflow: hidden;
    transition: border-color 0.2s;
    margin-bottom: 1rem;
  }
  .faq-item:hover {
    border-color: rgba(255, 107, 53, 0.3);
  }
  .faq-question {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 1.5rem;
    gap: 1rem;
  }
  .faq-question h3 {
    font-size: 1.2rem;
    font-weight: 600;
    margin: 0;
    color: var(--foreground);
  }
  .faq-question i {
    color: var(--muted-foreground);
    transition: transform 0.3s;
    flex-shrink: 0;
  }
  .faq-item.open .faq-question i {
    transform: rotate(90deg);
  }
  .faq-answer {
    display: none;
    padding: 0 1.5rem 1.5rem;
  }
  .faq-item.open .faq-answer {
    display: block;
  }
  .faq-answer p {
    font-size: 0.875rem;
    color: var(--muted-foreground);
    line-height: 1.7;
    margin: 0;
  }
</style>
